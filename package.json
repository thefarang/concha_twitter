{
  "name": "concha-twitter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/standard \"src/**/*.js\"",
    "fix-style": "./node_modules/.bin/standard --fix \"src/**/*.js\"",
    "debug": "./node_modules/.bin/nodemon ./bin/www",
    "test": "./node_modules/.bin/mocha --timeout 10000 \"test/routes/*.js\"",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --timeout 10000 \"test/routes/*.js\"",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "express": "4.15.5",
    "mongoose": "4.11.12"
  },
  "devDependencies": {
    "nodemon": "1.12.1",
    "standard": "10.0.3",
    "mocha": "3.5.3",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "istanbul": "0.4.5",
    "coveralls": "3.0.0",
    "mocha-lcov-reporter": "1.3.0"
  }
}
